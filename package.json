{
  "name": "@rafaelrinaldi/hn-cli",
  "version": "1.2.1",
  "description": "CLI to browse Hacker News",
  "license": "MIT",
  "repository": "rafaelrinaldi/hn-cli",
  "bugs": {
    "url": "https://github.com/rafaelrinaldi/hn-cli/issues"
  },
  "homepage": "https://github.com/rafaelrinaldi/hn-cli#readme",
  "author": {
    "name": "Rafael Rinaldi",
    "email": "rafael@rinaldi.io",
    "url": "rinaldi.io"
  },
  "bin": {
    "hn": "./bin/hn-cli"
  },
  "engines": {
    "node": ">=4"
  },
  "xo": {
    "esnext": true,
    "space": true,
    "rules": {
      "space-before-function-paren": [
        2,
        "never"
      ]
    }
  },
  "scripts": {
    "lint": "xo",
    "test": "npm run lint"
  },
  "keywords": [
    "cli",
    "hn",
    "hacker",
    "news",
    "feed",
    "rss",
    "reader",
    "app"
  ],
  "dependencies": {
    "blessed": "^0.1.81",
    "got": "^6.1.1",
    "log-update": "^1.0.2",
    "minimist": "^1.2.0",
    "moment": "^2.11.1",
    "multiline": "^1.0.2",
    "openurl": "^1.1.1",
    "pinkie-promise": "^2.0.0"
  },
  "devDependencies": {
    "xo": "^0.12.1"
  }
}
